{
  "expo": {
    "name": "Ayraç",
    "slug": "ayrac",
    "version": "1.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icon.png",
    "scheme": "ayrac",
    "backgroundColor": "#F7F6EE",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/images/notification_icon.png",
      "color": "#3b82f6"
    },
    "splash": {
      "image": "./assets/images/icon.png",
      "backgroundColor": "#F7F6EE",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon.png"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 29,
            "usesCleartextTraffic": false
          }
        }
      ],
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Kitap kapağı seçmek için galeriye erişim izni gerekiyor.",
          "cameraPermission": "Kitap kapağı çekmek için kameraya erişim izni gerekiyor."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Barkod taramak için kameraya erişim izni gerekiyor."
        }
      ],
      "expo-web-browser",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-6843300284653843~6479203818",
          "iosAppId": "ca-app-pub-6843300284653843~6479203818"
        }
      ],
      "expo-secure-store",
      "@react-native-firebase/app",
      "@react-native-firebase/perf",
      "@react-native-firebase/crashlytics"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-6843300284653843~6479203818"
    },
    "android": {
      "versionCode": 1,
      "backgroundColor": "#F7F6EE",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#F7F6EE"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.RECORD_AUDIO"
      ],
      "blockedPermissions": ["android.permission.RECORD_AUDIO"],
      "allowBackup": false,
      "package": "com.ayrac.app",
      "googleServicesFile": "./google-services.json"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "790788de-2c32-4e77-8409-762e23d3ee76"
      }
    },
    "owner": "chelebyyy"
  }
}
